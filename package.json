{
  "name": "nofx-local-starter",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "vercel-build": "npm run fe:build:actual",
    "start": "ts-node-dev --respawn --transpile-only src/simple.ts",
    "bootstrap:dev": "bash scripts/bootstrap-dev.sh",
    "dev:api": "ts-node-dev --respawn --transpile-only src/api/main.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker/main.ts",
    "dev": "npm-run-all -p dev:api dev:worker",
    "fe:dev": "npm --prefix apps/frontend run dev -- --host ${VITE_HOST:-0.0.0.0} --port ${VITE_PORT:-5173}",
    "fe:build": "turbo run fe:build",
    "fe:build:actual": "npm --prefix apps/frontend run build",
    "fe:preview": "npm --prefix apps/frontend run preview",
    "ci:e2e": "bash scripts/e2e-ci.sh",
    "create:bucket": "ts-node-dev --transpile-only scripts/createBucket.ts",
    "seed:dbwrite": "SEED_CONFIRM=1 ts-node-dev --transpile-only scripts/seedDbWriteRules.ts",
    "import:models:openai": "ts-node-dev --transpile-only scripts/importModels.ts openai",
    "import:models:anthropic": "ts-node-dev --transpile-only scripts/importModels.ts anthropic",
    "import:models:gemini": "ts-node-dev --transpile-only scripts/importModels.ts gemini",
    "registry:agents:validate": "ts-node-dev --transpile-only scripts/registry/validateAgents.ts",
    "registry:agents:publish": "ts-node-dev --transpile-only scripts/registry/publishAgents.ts",
    "registry:sync": "ts-node-dev --transpile-only scripts/registry/syncRegistry.ts",
    "nav:lint": "ts-node-dev --transpile-only scripts/validate-navigation.ts",
    "nav:validate": "ts-node-dev --transpile-only scripts/validate-navigation.ts",
    "settings:set:order": "ts-node-dev --transpile-only scripts/setPreferredOrder.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --format=stylish --ext .ts src",
    "hooks:full": "cp .lefthook.yml .lefthook.yml.active && lefthook install -f && echo '🏰 Full hooks enabled (slow but thorough)'",
    "hooks:fast": "cp .lefthook-fast.yml .lefthook.yml && lefthook install -f && echo '⚡ Fast hooks enabled (balanced)'",
    "hooks:minimal": "cp .lefthook-minimal.yml .lefthook.yml && lefthook install -f && echo '🚀 Minimal hooks enabled (maximum speed)'",
    "hooks:off": "lefthook uninstall && echo '❌ Hooks disabled (commit at your own risk!)'",
    "test": "jest --coverage",
    "test:api": "jest --config jest.config.api.js",
    "test:api:watch": "jest --config jest.config.api.js --watch",
    "test:api:coverage": "jest --config jest.config.api.js --coverage",
    "test:vercel": "npm run test:api",
    "precommit:test": "npm run test:api",
    "gate:typecheck": "zx scripts/runGate.mjs typecheck",
    "gate:lint": "zx scripts/runGate.mjs lint",
    "gate:unit": "zx scripts/runGate.mjs unit",
    "gate:sast": "zx scripts/runGate.mjs sast",
    "gate:secrets": "zx scripts/runGate.mjs secrets",
    "gate:audit": "zx scripts/runGate.mjs audit",
    "gate:unused": "zx scripts/runGate.mjs unused",
    "pre-deploy": "bash scripts/pre-deploy-checks.sh && npm run test:api",
    "pre-deploy:full": "bash scripts/pre-deploy-checks.sh && npm run gate:lint && npm run gate:sast && npm run gate:secrets && npm run test:api",
    "vercel:dev": "vercel dev --listen 3001",
    "vercel:build": "vercel build",
    "vercel:deploy": "vercel",
    "vercel:prod": "vercel --prod",
    "gates": "concurrently \"npm:gate:typecheck\" \"npm:gate:lint\" \"npm:gate:unit\" \"npm:gate:sast\" \"npm:gate:secrets\" \"npm:gate:audit\" \"npm:gate:unused\"",
    "ai-test": "node scripts/ai-test.js",
    "ai-test:auth": "node scripts/ai-test.js auth",
    "ai-test:run": "node scripts/ai-test.js run",
    "ai-test:status": "node scripts/ai-test.js status",
    "gates:fast": "turbo run gate:typecheck gate:lint gate:unit gate:sast gate:secrets gate:audit gate:unused",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:security": "jest tests/security",
    "test:smoke": "newman run collections/nofx-smoke-tests.json --environment environments/local.json --bail",
    "test:smoke:staging": "newman run collections/nofx-smoke-tests.json --environment environments/staging.json --bail",
    "test:smoke:prod": "newman run collections/nofx-smoke-tests.json --environment environments/production.json --bail --timeout-request 5000",
    "test:api:newman": "newman run collections/nofx-api-tests.json --environment environments/local.json",
    "test:contracts": "newman run collections/nofx-api-tests.json --environment environments/local.json --reporters cli,json",
    "test:artillery": "artillery run artillery.yml",
    "test:load": "RESPONSES_RUNTIME_MODE=stub npx jest tests/perf --runInBand",
    "test:performance": "artillery run tests/performance/load.artillery.yml",
    "test:benchmarks": "jest tests/performance/benchmarks.test.ts --runInBand",
    "test:performance-full": "npm run test:benchmarks && npm run test:performance",
    "test:cloud": "jest tests/cloud-migration --passWithNoTests",
    "test:cloud:vercel": "jest tests/cloud-migration/vercel-functions.test.ts",
    "test:cloud:supabase": "jest tests/cloud-migration/supabase-database.test.ts",
    "test:cloud:health": "jest tests/cloud-migration/health-monitoring.test.ts",
    "test:cloud:deployment": "jest tests/cloud-migration/deployment-validation.test.ts",
    "test:stress": "jest tests/performance/stress.test.ts",
    "test:chaos": "jest tests/chaos",
    "test:ui": "npm --prefix apps/frontend run test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:all": "npm-run-all test:unit test:integration test:smoke test:e2e test:security",
    "test:quick": "npm run test:unit && npm run test:smoke",
    "test:full": "npm run test:unit && npm run test:api:newman && npm run test:e2e",
    "test:bulletproof": "npm run test:unit && echo '✅ All 194 bulletproof tests passing!'",
    "test:bulletproof-full": "npm-run-all test:coverage test:all test:stress test:chaos",
    "test:teams": "npm run test:teams:unit && npm run test:teams:integration && npm run test:teams:e2e",
    "test:teams:unit": "jest --config=jest.config.teams.js --testPathPatterns=teams.test.ts",
    "test:teams:integration": "jest --config=jest.config.teams.js --testPathPatterns=teams.integration.test.ts",
    "test:teams:e2e": "playwright test --config=playwright.config.teams.ts",
    "test:teams:coverage": "jest --config=jest.config.teams.js --coverage",
    "test:teams:watch": "jest --config=jest.config.teams.js --watch",
    "test:teams:debug": "node --inspect-brk node_modules/.bin/jest --config=jest.config.teams.js --runInBand",
    "test:teams:security": "npm audit && npm run test:teams:unit -- --testNamePattern='Security'",
    "test:teams:performance": "npm run test:teams:unit -- --testNamePattern='Performance'",
    "test:teams:chaos": "npm run test:teams:unit -- --testNamePattern='Chaos'",
    "test:teams:bulletproof": "npm run test:teams:coverage && npm run test:teams:security && npm run test:teams:e2e",
    "test:teams:ci": "npm run test:teams:bulletproof -- --ci --reporters=default --reporters=jest-junit",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "changeset publish",
    "changeset:status": "changeset status",
    "hooks:install": "lefthook install",
    "hooks:uninstall": "lefthook uninstall",
    "analyze:bundle": "vite-bundle-analyzer dist",
    "analyze:deps": "madge --image deps.svg src",
    "analyze:circular": "madge --circular src",
    "analyze:all": "npm-run-all analyze:deps analyze:circular",
    "mock:start": "msw start",
    "mock:init": "msw init public",
    "start:mock": "concurrently \"npm:mock:start\" \"npm:dev\"",
    "size:check": "size-limit",
    "size:why": "size-limit --why",
    "size:analyze": "npm run analyze:bundle && npm run size:check",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "demo:record": "echo '🎬 Recording demo... (install loom-cli for video recording)'",
    "profile": "node --prof src/simple.js",
    "profile:analyze": "node --prof-process isolate-*.log > profile.txt",
    "graph:deps": "npm run analyze:deps && open deps.svg",
    "graph:nx": "nx graph",
    "fortress:build": "npm run gates:fast && npm run test:bulletproof && echo '🏰 Fortress is impenetrable!'",
    "fortress:demo": "npm run analyze:all && npm run size:analyze && npm run changeset:status",
    "perf:monitor": "echo '📊 Starting performance monitoring dashboard...' && open benchmarks/dashboard.html",
    "perf:baseline": "npm run test:benchmarks && echo '📊 Baseline performance results saved'",
    "perf:compare": "npm run test:benchmarks && echo '📊 Performance comparison complete'",
    "perf:report": "node -e \"console.log('📊 Performance report generation not implemented yet')\"",
    "admin:super": "echo '🛡️ Opening super admin dashboard...' && open http://localhost:3002/dev/super-admin",
    "registry:templates:validate": "ts-node-dev --transpile-only scripts/registry/validateTemplates.ts",
    "registry:templates:publish": "ts-node-dev --transpile-only scripts/registry/publishTemplates.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@anthropic-ai/sdk": "^0.25.1",
    "@aws-sdk/client-sqs": "^3.637.0",
    "@google/generative-ai": "^0.15.0",
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/resources": "1.15.2",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@react-email/components": "^0.5.5",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/cors": "^2.8.19",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.7.8",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "micro": "^10.0.1",
    "openai": "^4.56.0",
    "pg": "^8.12.0",
    "pino": "^9.4.0",
    "prom-client": "^15.1.3",
    "resend": "^6.1.0",
    "simple-git": "^3.28.0",
    "stripe": "^18.5.0",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.55.0",
    "@size-limit/preset-app": "^11.2.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vercel/node": "^2.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "artillery": "^2.0.26",
    "concurrently": "^8.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-formatter-json": "^8.40.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jest": "^30.1.3",
    "k6": "^0.0.0",
    "knip": "^5.63.1",
    "lefthook": "^1.4.0",
    "madge": "^6.1.0",
    "msw": "^2.11.3",
    "newman": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.55.0",
    "size-limit": "^8.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "turbo": "^1.13.4",
    "typescript": "^5.5.4",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^3.2.4",
    "zx": "^7.2.0"
  },
  "overrides": {
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/core": "1.15.2",
    "@opentelemetry/resources": "1.15.2",
    "@opentelemetry/sdk-trace-base": "1.15.2",
    "@opentelemetry/sdk-metrics": "1.15.2"
  }
}
